.gallery4__images
	margin 0
	padding 0
	list-style none

.gallery4__image
	width 100%
	margin-left 20px

.gallery4__photo, .gallery4__video
	display block
	object-fit cover
	margin-bottom 12px
	width 100%
	height 195px

	@media(min-width tablet)
		margin-bottom 16px
		height 420px

	@media(min-width desktop)
		margin-bottom 20px
		height 750px

.gallery4__name
	margin 0 0 8px
